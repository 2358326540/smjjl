}
a:active {
	background-color: transparent
}
a:active, a:hover {
	outline: 0 none
}
a:focus {
	outline: 1px dotted
}
input[type=”button”], input[type=”submit”], input[type=”reset”] {
	-webkit-appearance: none
}
.clearfix:before, .clearfix:after {
